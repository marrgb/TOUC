<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		id="logoff"
		title="{i18n>logoffTitle}">

		<beginButton>
    <Button
      text="{i18n>btnSi}"
      press="onLogoff"
      class="sapUiSmallMarginEnd"/>
		</beginButton>

		<endButton>
		<Button
      text="{i18n>btnCancel}"
      press="onCloseDialog"
      class="sapUiSmallMarginEnd"/>
		</endButton>

		<content>
					<Text text="{i18n>logofText}"/>
		</content>
	</Dialog>


</core:FragmentDefinition>
